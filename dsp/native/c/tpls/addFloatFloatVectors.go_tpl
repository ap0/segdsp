//+build !noasm
//+build !appengine

package {PACKAGE}

import (
  "unsafe"
)

//go:noescape
func _addFloatFloatVectors{SUBARCHTITLE}(A, B unsafe.Pointer, length uint)

func AddFloatFloatVectors{SUBARCHTITLE}(A, B []float32) {{
  var aPtr = unsafe.Pointer(&A[0])
  var bPtr = unsafe.Pointer(&B[0])
  var cLen = uint(len(A))

  if cLen > uint(len(B)) {{
    cLen = uint(len(B))
  }}

  _addFloatFloatVectors{SUBARCHTITLE}(aPtr, bPtr, cLen)
}}
